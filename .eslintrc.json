{
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "indent": 2,
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-underscore-dangle": 0,
    "brace-style": [1, "stroustrup"]
  }
}